<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.urclean">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">

        <activity android:name=".solicitudCambioTurno"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".MenuSupervisorActivity"></activity>
        <activity android:name=".Tarea_Concreta"></activity>
        <activity android:name=".tareasBarrendero" />
        <activity android:name=".AccesoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuCiudadanoActivity" />
        <activity android:name=".menuBarrendero" />
    </application>

</manifest>